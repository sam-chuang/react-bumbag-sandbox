{
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env NODE_ENV=production snowpack dev --reload",
    "build": "cross-env NODE_ENV=production snowpack build",
    "test": "web-test-runner \"src/**/*.test.jsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "tag:docker:dev": "cross-env-shell GIT_COMMIT_HASH=$(git show --no-patch --format=%h) \"npm-run-all build:docker:dev push:docker:dev\"",
    "build:docker:dev": "cross-var docker build . -t registry.gitlab.com/rayprosoft/turnkey-gaming-system/mocksite:$npm_package_version-$GIT_COMMIT_HASH",
    "push:docker:dev": "docker push registry.gitlab.com/rayprosoft/turnkey-gaming-system/mocksite:$npm_package_version-$GIT_COMMIT_HASH"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^11.2.5",
    "@web/test-runner": "^0.12.17",
    "bumbag": "^1.11.4",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "http-proxy": "^1.18.1",
    "husky": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-hook-form": "^6.15.5",
    "react-router-dom": "^5.2.0",
    "snowpack": "3.0.13",
    "swr": "^0.5.4"
  }
}
